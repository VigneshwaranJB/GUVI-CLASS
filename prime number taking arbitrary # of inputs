lst = [] 
n = int(input("Enter number of elements : ")) 
for i in range(0, n): 
    element = int(input())   
    lst.append(element)
      
print(lst) 
for num in lst:
    if num > 1:
        for i in range(2, num):
            if (num % i) == 0:
                break
        else:
            print("The number {} is a prime number".format(num))
